            <Font size="26.0" />
         </font>
      </Button>
      <Pane layoutX="282.0" layoutY="713.0" prefHeight="200.0" prefWidth="825.0" />
      <ImageView fitHeight="80.0" fitWidth="80.0" layoutX="710.0" layoutY="546.0" pickOnBounds="true" preserveRatio="true" />
   </children>
